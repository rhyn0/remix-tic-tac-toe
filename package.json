{
    "name": "remix-tic-tac-toe",
    "private": true,
    "sideEffects": false,
    "type": "module",
    "version": "0.1.0",
    "scripts": {
        "build": "remix vite:build",
        "dev": "remix vite:dev",
        "start": "remix-serve ./build/server/index.js",
        "prepare": "husky",
        "lint": "npm run lint:rules && npm run lint:format && npm run lint:types",
        "lint:types": "tsc --noEmit",
        "lint:format": "biome format --error-on-warnings ./",
        "lint:rules": "biome lint --error-on-warnings ./",
        "fix": "npm run fix:format && npm run fix:lint",
        "fix:format": "biome format --write ./",
        "fix:lint": "biome lint --write ./",
        "db:generate": "drizzle-kit generate",
        "db:push": "drizzle-kit push",
        "db:migrate": "drizzle-kit migrate",
        "pre-commit": "./.husky/pre-commit"
    },
    "dependencies": {
        "@radix-ui/react-dropdown-menu": "2.0.6",
        "@radix-ui/react-label": "^2.0.2",
        "@radix-ui/react-slot": "^1.0.2",
        "@remix-run/node": "^2.9.2",
        "@remix-run/react": "^2.9.2",
        "@remix-run/serve": "^2.9.2",
        "better-sqlite3": "^11.0.0",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.1",
        "drizzle-orm": "^0.31.2",
        "isbot": "^4.1.0",
        "lucide-react": "^0.381.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tailwind-merge": "^2.3.0",
        "tailwindcss-animate": "^1.0.7",
        "tiny-invariant": "^1.3.3",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.8.3",
        "@remix-run/dev": "^2.9.2",
        "@types/better-sqlite3": "^7.6.10",
        "@types/react": "^18.2.20",
        "@types/react-dom": "^18.2.7",
        "autoprefixer": "^10.4.19",
        "drizzle-kit": "^0.22.6",
        "husky": "^9.0.11",
        "postcss": "^8.4.38",
        "tailwindcss": "^3.4.3",
        "typescript": "^5.4.5",
        "vite": "^5.1.0",
        "vite-tsconfig-paths": "^4.2.1"
    },
    "engines": {
        "node": ">=20.0.0"
    }
}
